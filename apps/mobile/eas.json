{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "preview": {
      "channel": "preview"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
