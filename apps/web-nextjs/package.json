    "db:drop:dev": "NODE_ENV=development drizzle-kit drop --config=drizzle-dev.config.ts",
    "db:seed": "NODE_ENV=production bun run scripts/setup-turso.ts",
    "db:seed:dev": "NODE_ENV=development bun run scripts/setup-turso.ts",
    "db:studio": "NODE_ENV=development drizzle-kit studio --config=drizzle-dev.config.ts",
