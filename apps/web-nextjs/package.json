    "db:migrate": "NODE_ENV=production drizzle-kit migrate --config=drizzle-prod.config.ts",
    "db:migrate:dev": "NODE_ENV=development drizzle-kit migrate --config=drizzle-dev.config.ts",
    "db:push": "NODE_ENV=production drizzle-kit push --config=drizzle-prod.config.ts",
    "db:push:dev": "NODE_ENV=development drizzle-kit push --config=drizzle-dev.config.ts",
